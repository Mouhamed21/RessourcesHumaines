{% extends 'base.html.twig' %}
{% block menu %}
    {% include 'carrieres/inc/sidebare.html.twig' %}
{% endblock %}
{% block content %}
    <section class="content-header">
        <h1>
            Carrières et Compétences
            <small>Evaluation du Personnel</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-edit"></i>Carrières et Compétences</a></li>
            <li class="active">Evaluation du Personnel</li>
        </ol>
    </section>
{% endblock %}

{% block body %}
    <h1>Contact</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ contact.id }}</td>
            </tr>
            <tr>
                <th>Nomcontact</th>
                <td>{{ contact.nomcontact }}</td>
            </tr>
            <tr>
                <th>Prenomcontact</th>
                <td>{{ contact.prenomcontact }}</td>
            </tr>
            <tr>
                <th>Sexecontact</th>
                <td>{{ contact.sexecontact }}</td>
            </tr>
            <tr>
                <th>Emailcontact</th>
                <td>{{ contact.emailcontact }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ contact.telephone }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('informationsdiverses_contact_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('informationsdiverses_contact_edit', { 'id': contact.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
